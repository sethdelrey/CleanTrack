@model SuppliesModel

 <div class="card card-primary">
     <div class="card-header">
         <h2>Needed Supplies</h2>
     </div>
     <div class="card-body">
         <ul>
             @foreach (var item in Model.Supplies)
             {
                 <li>
                     @item.Name
                 </li>
             }
         </ul>

         <br />
         <a asp-controller="Supplies" asp-action="EditList" >Edit Supplies Need</a>

     </div>
 </div>